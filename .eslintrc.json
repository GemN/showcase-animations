{
  "parser": "babel-eslint",
  "plugins": ["flowtype"],
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "import/order": ["error", {"newlines-between": "always"}],
    "max-len": ["error", { "code": 120 }]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
